    "gulp-filter": "4.0.0",
    "gulp-jade": "1.1.0",
    "gulp-jasmine": "2.4.2",
    "gulp-jshint": "2.0.4",
    "gulp-less": "3.3.0",
    "gulp-mocha": "3.0.1",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "1.2.2",
    "gulp-strip-debug": "1.1.0",
    "gulp-uglify": "2.0.1",
    "jshint": "2.9.4",
